      <!-- jQuery Star rating -->
      <script src="[% session.app_url %]/admin/js/jquery.rateit.min.js"></script>
      <!-- prettyPhoto -->
      <script src="[% session.app_url %]/admin/js/jquery.prettyPhoto.js"></script>  
      
      <!-- jQuery flot -->
